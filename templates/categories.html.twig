  {% block main %}

      {% for category in categories %}
         {% if category.published %}
             {% endif %}
                <article>
                    <a href={{ path('categoryShow', {'id' : category.id}) }}>
                        <p>{{ category.title }}</p>
                        <p>{{ category.content }}</p>
                    </a>
                </article>
      {% endfor %}

  {% endblock main %}