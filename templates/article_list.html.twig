{% extends 'base.html.twig' %}

{% block metaTitle %}
    Titre
{% endblock metaTitle %}

{% block main %}

    {% for article in articles %}
        <article class="article">
                <h2>{{ article.title }}</h2>
                <p>{{ article.content }}</p>
                <p>{{ article.createdAt | date('y-m-d') }}</p>
        </article>
    {% endfor %}

{% endblock main %}

